@using Newtonsoft.Json;
@model IEnumerable<Medical.Models.Data.Nhapkho>
@{
    ViewBag.Title = "Nhapkho";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/Custom/Duoc.css" rel="stylesheet" />
<div class="container-fluid">
    <!-- Nav tabs -->
    <ul class="nav nav-tabs" role="tablist">
        <li role="presentation"><a href="#chitiet" aria-controls="chitiet" role="tab" data-toggle="tab">Chi tiết phiếu</a></li>
        <li role="presentation" class="active"><a href="#danhsach" aria-controls="danhsach" role="tab" data-toggle="tab">Danh sách phiếu nhập</a></li>
    </ul>
    <!-- Tab panes -->
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane fade" id="chitiet">
            <div class="row custom-row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="container-fluid custom-title">Chi tiết phiếu</div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_ngay", "datepicker", "Ngày"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_lydo", "filter", "Lý do"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_ngayhd", "datepicker", "Ngày hóa đơn"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sohd", "input", "Số hóa đơn"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_kho", "filter", "Kho nhập"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_nhacc", "filter", "Nhà cung cấp"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_ngayn", "datepicker", "Ngày nhập"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sophieu", "input", "Số phiếu"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_ngaykn", "datepicker", "Ngày kiểm nhập"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sokn", "input", "Số kiểm nhập"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_nguoigiao", "input", "Người giao"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_ghichu", "input", "Ghi chú"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sotien", "input", "Số tiền"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sotientruck", "input", "Số tiền(sau CK)"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sotientruckcongvat", "input", "Số tiền(Sau CK + VAT)"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="row">
                        <script type="text/javascript">
                            document.write(f_create_table_html('@Html.Raw(JsonConvert.SerializeObject(Model).Replace("'", "\\'"))', "@ViewData["page"]", "ID~Nhóm~Mã~Tên~Ghi chú", "id~nhomkho~ma~ten~ghichu", false));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_nguon", "filter", "Nguồn nhập"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_sodk", "input", "Sô ĐK"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-5 col-md-5 col-sm-5 col-xs-5">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_duoc", "filter", "Tên thuốc"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_hoatchat", "filter", "Hoạt chất"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_ct_hangsx", "filter", "Hãng SX"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row custom-btn-group">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="text-align:right">
                    <script type="text/javascript">
                        document.write(f_create_btn_group("@ViewData["page"]_ct", "themmoi~luu~boqua~sua~xoa~ketthuc", "primary~primary~primary~primary~primary~primary", "Thêm mới~Lưu~Bỏ Qua~Sữa~Xóa~Kết Thúc"));//success~primary~info~warning~danger~default
                    </script>
                </div>
            </div>            
        </div>
        <div role="tabpanel" class="tab-pane fade  in active" id="danhsach">
            <div class="row custom-row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="container-fluid custom-title">Lọc phiếu</div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_lydo", "filter", "Lý do"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_tn", "datepicker", "Từ"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_dn", "datepicker", "Đến"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_kho", "filter", "Tại kho"));
                        </script>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                    <div class="input-group">
                        <script type="text/javascript">
                            document.write(f_create_input("@ViewData["page"]_nhanvien", "filter", "Nhân viên"));
                        </script>
                    </div>
                </div>
            </div>
            <div class="row custom-row custom-btn-group">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="text-align:left">
                    <script type="text/javascript">
                        document.write(f_create_btn_group("@ViewData["page"]", "tim~themmoi", "primary~primary", "Tìm~Thêm mới"));//success~primary~info~warning~danger~default
                    </script>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="container-fluid custom-title">Danh sách phiếu nhập</div>
                </div>
            </div>
            <div class="row custom-row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="row">
                        <script type="text/javascript">
                            document.write(f_create_table_html('@Html.Raw(JsonConvert.SerializeObject(Model).Replace("'", "\\'"))', "@ViewData["page"]", "ID~Nhóm~Mã~Tên~Ghi chú", "id~nhomkho~ma~ten~ghichu", false));
                        </script>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/Custom/Duoc.js"></script>
